{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "RethinkDbDev": {
    "Application": "TokenGen.Debug",
    "Host": "localhost",
    "Port": 28015,
    "Timeout": 10,
    "Database": "TokenStore"
  },
  "RethinkDbStaging": {
    "Application": "TokenGen.Staging",
    "Host": "rdb-proxy",
    "Port": 28015,
    "Timeout": 10,
    "Database": "TokenStore"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "IpWhitelist": [ "192.168.0.0/24" ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 10
      },
      {
        "Endpoint": "*",
        "Period": "15m",
        "Limit": 1000
      }
    ]
  }
}
